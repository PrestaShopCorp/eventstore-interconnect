{
  "version": 2,
  "projects": {
    "eventstore-interconnect": {
      "root": "libs/eventstore-interconnect",
      "sourceRoot": "libs/eventstore-interconnect/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/libs/eventstore-interconnect",
            "tsConfig": "libs/eventstore-interconnect/tsconfig.lib.json",
            "packageJson": "libs/eventstore-interconnect/package.json",
            "main": "libs/eventstore-interconnect/src/index.ts",
            "assets": [
              "libs/eventstore-interconnect/*.md"
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "libs/eventstore-interconnect/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/eventstore-interconnect"
          ],
          "options": {
            "jestConfig": "libs/eventstore-interconnect/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "examples": {
      "root": "apps/examples",
      "sourceRoot": "apps/examples/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/examples",
            "main": "apps/examples/src/main.ts",
            "tsConfig": "apps/examples/tsconfig.app.json",
            "assets": [
              "apps/examples/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/examples/src/environments/environment.ts",
                  "with": "apps/examples/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "examples:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/examples/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/examples"
          ],
          "options": {
            "jestConfig": "apps/examples/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "nestjs-geteventstore-legacy-proxy": {
      "root": "libs/nestjs-geteventstore-legacy-proxy",
      "sourceRoot": "libs/nestjs-geteventstore-legacy-proxy/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/libs/nestjs-geteventstore-legacy-proxy",
            "tsConfig": "libs/nestjs-geteventstore-legacy-proxy/tsconfig.lib.json",
            "packageJson": "libs/nestjs-geteventstore-legacy-proxy/package.json",
            "main": "libs/nestjs-geteventstore-legacy-proxy/src/index.ts",
            "assets": [
              "libs/nestjs-geteventstore-legacy-proxy/*.md"
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "libs/nestjs-geteventstore-legacy-proxy/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/nestjs-geteventstore-legacy-proxy"
          ],
          "options": {
            "jestConfig": "libs/nestjs-geteventstore-legacy-proxy/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "defaultProject": "examples"
}
